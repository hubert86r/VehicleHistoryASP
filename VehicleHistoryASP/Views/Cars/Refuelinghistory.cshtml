@model List<RefuelingHistory>

<h1>Historia tankowania</h1>
<br />

<table class="table">
    <thead class="thead-light">
    <tr>
        <th>Data</th>
        <th>Ilość</th>
        <th>Cena</th>
        <th>Koszt</th>
        <th>Przebieg</th>
        <th>Spalanie</th>
    </tr>
    <tr>
        <th></th>
        <th>[l]</th>
        <th>[zł]</th>
        <th>[zł]</th>
        <th>[km]</th>
        <th>[l/100km]</th>
    </tr>
    @foreach (var st in Model)
    {
        <tr>
            <td>@st.date</td>
            <td>@st.fuelQquantity</td>
            <td>@st.price</td>
            <td>@st.cost</td>
            <td>@st.milage</td>
            <td>@st.fuelUsage</td>
        </tr>
    }
</table>
